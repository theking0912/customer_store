.content {
  margin-top: 10px;
  display: flex;
  flex-direction: row;
}

.item-left {
  width: 10%;
  height: 50px;
  margin-left: 10px;
  padding-top: 5px;
}

.item-mid {
  flex: 1;
  width: 60%;
  height: 50px;
  margin-left: 10px;
  padding-top: 5px;
}

.item-right {
  width: 15%;
  height: 50px;
  margin-left: 10px;
  padding-top: 5px;
}

.item-image {
  width: 50px;
  height: 50px;
}

.item-font {
  font-size: large;
  text-align: center;

}
